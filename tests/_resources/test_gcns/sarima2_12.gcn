options
{
    linear = True;
};


block STEADY_STATE
{
    identities
    {
        x[ss] = 0.0;
    };
};

block SARIMA_2_12
{
    identities
    {
        x[] = rho_1 * x[-1] + rho_2 * x[-2]  # AR(2) part
            + rho_12 * x[-12]  # Seasonal AR(1) part
            + rho_1 * rho_12 * x[-13] + rho_12 * rho_2 * x[-14] # Interaction terms
            + epsilon[];
    };

    shocks
    {
        epsilon[] ~ Normal(mu=0, sigma=sigma_epsilon);
    };

    calibration
    {
        rho_1  ~ Normal(mu=0.9, sigma=0.1) = 0.95;
        rho_2  ~ Normal(mu=-0.3, sigma=0.1) = -0.4;
        rho_12 ~ Normal(mu=0.2, sigma=0.1) = 0.2;
        sigma_epsilon ~ HalfNormal(sigma=1) = 1.0;
    };
};
